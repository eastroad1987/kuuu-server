{
  "name": "kuuu-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist && echo 'dist 디렉토리 삭제 완료'",
    "build": "nest build && echo '빌드 중 dist 디렉토리 내용:' && ls -la dist && echo 'src 디렉토리 내용:' && ls -la src",
    "postbuild1": "echo '빌드 후 dist 디렉토리 내용:' && ls -la dist",
    "vercel-build": "npm run prebuild && npm run build && npm run postbuild1 && mkdir -p api && npm run postbuild2 && npm run afterbuild",
    "postbuild2": "echo '빌드 후 dist 디렉토리 내용:' && ls -la dist",
    "afterbuild": "cp dist/main.js api/v1.js && echo '파일 복사 완료'",
    "start": "node dist/main.js",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "cross-env NODE_ENV=dev node --max-old-space-size=1024 node_modules/.bin/nest start --watch",
    "start:dev": "node --max-old-space-size=1024 node_modules/.bin/nest start --watch",
    "start:debug": "node --max-old-space-size=1024 node_modules/.bin/nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy": "vercel --prod",
    "deploy:debug": "vercel --debug"
  },
  "dependencies": {
    "@nestjs/axios": "^3.1.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/testing": "^10.3.3",
    "@nestjs/typeorm": "^10.0.2",
    "@types/multer-s3": "^3.0.3",
    "aws-sdk": "^2.1116.0",
    "axios": "^1.7.9",
    "cache-manager": "^6.3.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.11.5",
    "node-sens": "^1.3.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path-to-regexp": "^6.2.1",
    "pg": "^8.15.6",
    "query-string": "^9.1.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "serverless-http": "^3.2.0",
    "typeorm": "^0.3.20",
    "vercel": "^41.6.2",
    "webpack": "^5.97.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager": "^4.0.6",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vercel/node": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.7",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "request-ip": "^2.1.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}

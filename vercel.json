{
  "version": 2,
  "functions": {
    "api/v1.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/v1.js"
    }
  ],
  "regions": ["icn1"],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "api"
}

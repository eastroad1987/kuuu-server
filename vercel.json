{
  "version": 2,
  "functions": {
    "api/v1.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/v1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "regions": ["icn1"],
  "env": {
    "NODE_ENV": "production"
  }
}
